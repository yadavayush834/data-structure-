//kadane's algorithm
int main(){
  vector<int>arr = {7,-8900,1,56,78,-90,4};
  int maxi = arr[0];
  int res = arr[0];
  for(int i = 1 ; i< arr.size(); i++){
    maxi = max(arr[i],arr[i]+maxi);
    res = max(maxi,res);
  }
} 
