#include <iostream>
#include <vector>
using namespace std;

int rangeMinQuery(const vector<int>& arr, int L, int R) {
    int mn = arr[L];
    for (int i = L + 1; i <= R; i++)
        mn = min(mn, arr[i]);
    return mn;
}

int main() {
    vector<int> arr = {1, 3, 2, 7, 9, 11};
    cout << rangeMinQuery(arr, 1, 4) << endl;  // Output: 2
    return 0;
}
